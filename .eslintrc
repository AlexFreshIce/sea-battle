{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb/base"],

  "parserOptions": {
    "ecmaVersion": 2020
  },

  "rules": {
    "no-alert": "off",
    "func-names": "off",
    "no-plusplus": "off",
    "no-use-before-define": "off",
    "default-case": "off",
    "no-param-reassign": "off",
    "no-continue": "off"
  }
}
